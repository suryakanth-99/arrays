#include <stdio.h>

int main()
{
    int a[100],n,i,large,small;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    small=large=a[0];
    for(i=1;i<n;i++)
    {
        if(a[i]<small)
        small=a[i];
        if(a[i]>large)
        large=a[i];
    }
    printf("\nlarge=%d ,small= %d",large,small);
    
    

    return 0;
}
